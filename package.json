{
  "name": "germinatec-api",
  "version": "1.0.0",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev-ts": "npx ts-node-dev --respawn src/server.ts",
    "dev": "nodemon --ext ts --exec npx ts-node src/server.ts",
    "start-ts": "npx ts-node-dev --respawn src/server.ts",
    "start": "node dist/server.js",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev",
    "prisma:reset": "dotenv -e .env -- npx prisma migrate reset",
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
